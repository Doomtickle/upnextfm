{% extends 'base.html.twig' %}
{% block title %}{{ name }}{% endblock %}
{% block body %}
    <div id="mount" data-room="{{ name }}">
        <div class="up-loader">
            <svg class="up-loader__circular">
                <circle class="up-loader__path" cx="50" cy="50" r="20" fill="none" stroke-width="2" stroke-miterlimit="10"></circle>
            </svg>
        </div>
    </div>
{% endblock %}
{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('build/js/room.js') }}"></script>
{% endblock %}
